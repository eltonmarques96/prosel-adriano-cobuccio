{
   "name": "backend",
   "version": "1.0.0",
   "main": "index.js",
   "keywords": [
      "node",
      "nodejs",
      "typescript",
      "api"
   ],
   "scripts": {
      "build": "tsc",
      "start": "node dist/src/index.js",
      "dev": "ts-node-dev --transpile-only src/index.ts | pino-pretty -c",
      "test": "echo \"Error: no test specified\" && exit 1",
      "test:functional": "NODE_ENV=test jest --projects ./test --forceExit --runInBand",
      "lint": "eslint ./src ./test --ext .ts",
      "migration": "typeorm migration:run --dataSource ./src/data-source.ts",
      "lint:fix": "eslint ./src ./test --ext .ts -npm run typeorm-sync-fix",
      "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
      "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
      "typeorm": "typeorm-ts-node-commonjs",
      "typeorm-sync": "typeorm-codebase-sync addReferences --dataSource ./src/data-source.ts --migrations ./src/database/migrations --entities ./src/database/entities --subscribers ./src/database/subscribers",
      "posttypeorm": "npm run typeorm-sync"
   },
   "author": "Elton Marques",
   "license": "ISC",
   "description": "",
   "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
   "devDependencies": {
      "@types/config": "^3.3.5",
      "@types/cors": "^2.8.17",
      "@types/express": "^5.0.1",
      "@types/express-pino-logger": "^4.0.5",
      "@types/jest": "^29.5.14",
      "@types/jsonwebtoken": "^9.0.9",
      "@types/module-alias": "^2.0.4",
      "@types/node": "^22.14.0",
      "@types/nodemailer": "^6.4.17",
      "@types/supertest": "^6.0.3",
      "@typescript-eslint/eslint-plugin": "^8.29.1",
      "@typescript-eslint/parser": "^8.29.1",
      "eslint": "^9.24.0",
      "jest": "^29.7.0",
      "nodemailer-mock": "^2.0.8",
      "pino-pretty": "^13.0.0",
      "prettier": "^3.5.3",
      "supertest": "^7.1.0",
      "ts-jest": "^29.3.1",
      "ts-node": "^10.9.2",
      "ts-node-dev": "^2.0.0",
      "typeorm-codebase-sync": "^1.1.2",
      "typescript": "^5.8.3"
   },
   "dependencies": {
      "@overnightjs/core": "^1.7.6",
      "bcryptjs": "^3.0.2",
      "body-parser": "^2.2.0",
      "config": "^3.3.12",
      "cors": "^2.8.5",
      "dotenv": "^16.5.0",
      "express": "^5.1.0",
      "express-pino-logger": "^7.0.0",
      "jsonwebtoken": "^9.0.2",
      "module-alias": "^2.2.3",
      "nodemailer": "^6.10.0",
      "pg": "^8.14.1",
      "pino": "^9.6.0",
      "reflect-metadata": "^0.2.2",
      "sqlite3": "^5.1.7",
      "typeorm": "0.3.22",
      "ulid": "^3.0.0"
   }
}
